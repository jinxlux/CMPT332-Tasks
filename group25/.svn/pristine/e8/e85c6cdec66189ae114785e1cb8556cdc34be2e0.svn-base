#include "kernel/types.h"
#include "user/user.h"

int
main(int argc, char *argv[])
{
  int current_runnable, pid, n;
  print("sys_numprocs test\n");

  for(n=0; n<10; n++){
    pid = fork();
    if(pid < 0)
      break;
    if(pid == 0)
      exit();
  }

  if(n == 10){
    print("fork claimed to work 10 times!\n");
    exit();
  }


  

  current_runnable = numprocs();
  if (current_runnable < 0){
    printf("2", "numprocs failed\n");
    exit();
  }
  print("sys_numprocs test OK\n");

  exit();
}